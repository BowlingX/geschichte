{
  "name": "geschichte",
  "version": "0.1.0",
  "description": "manage query parameters with react-hooks",
  "main": "build/cjs/index.js",
  "typings": "build/mjs/index.d.ts",
  "module": "build/mjs/index.js",
  "exports": {
    ".": {
      "types": "./build/mjs/index.d.ts",
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js"
    },
    "./nextjs": {
      "types": "./build/mjs/lib/adapters/nextjs/index.d.ts",
      "import": "./build/mjs/lib/adapters/nextjs/index.js",
      "require": "./build/cjs/lib/adapters/nextjs/index.js"
    },
    "./historyjs": {
      "types": "./build/mjs/lib/adapters/historyjs/index.d.ts",
      "import": "./build/mjs/lib/adapters/historyjs/index.js",
      "require": "./build/cjs/lib/adapters/historyjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "build",
    "nextjs.d.ts",
    "historyjs.d.ts"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BowlingX/geschichte.git"
  },
  "bugs": {
    "url": "https://github.com/BowlingX/geschichte/issues"
  },
  "homepage": "https://github.com/BowlingX/geschichte#readme",
  "license": "MIT",
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "history",
    "url",
    "query-parameters"
  ],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:* && ./esm-postbuild.sh",
    "build:main": "tsc-esm -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.{ts,tsx}\" --list-different",
    "test:unit": "jest --coverage",
    "cov:send": "codecov",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "doc": "run-s doc:html && open-cli build/docs/api/index.html",
    "doc:html": "typedoc src/ --exclude \"**/+(__tests__|examples)/*\" --out build/docs/api",
    "doc:json": "typedoc src/ --exclude \"**/+(__tests__|examples)/*\" --json build/docs/api/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "clean": "trash build test",
    "storybook": "start-storybook -p 6006",
    "prepublishOnly": "run-s build",
    "build-storybook": "build-storybook -o build/docs"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "memoize-one": "^6.0.0"
  },
  "peerDependencies": {
    "history": "^5",
    "immer": ">=7",
    "next": ">=11",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "zustand": "^4"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "history": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@digitak/tsc-esm": "^3.1.2",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-docs": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.5",
    "browser-env": "^3.3.0",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "expect": "^28.1.0",
    "gh-pages": "^4.0.0",
    "history": "^5.3.0",
    "immer": "^9.0.14",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-junit": "^13.2.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "next": "^12",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.1.0",
    "semantic-release": "^19.0.2",
    "trash-cli": "^5.0.0",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "zustand": "^4.0.0-rc.1"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "dot-prop": "5.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/__tests__/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "roots": [
      "src/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
