{
  "name": "geschichte",
  "version": "0.1.0",
  "description": "manage query parameters with react-hooks",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BowlingX/geschichte.git"
  },
  "bugs": {
    "url": "https://github.com/BowlingX/geschichte/issues"
  },
  "homepage": "https://github.com/BowlingX/geschichte#readme",
  "license": "MIT",
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "history",
    "url",
    "query-parameters"
  ],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.{ts,tsx}\" --list-different",
    "test:unit": "jest --coverage",
    "cov:send": "codecov",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "doc": "run-s doc:html && open-cli build/docs/api/index.html",
    "doc:html": "typedoc src/ --exclude \"**/+(__tests__|examples)/*\" --target ES6 --mode file --out build/docs/api",
    "doc:json": "typedoc src/ --exclude \"**/+(__tests__|examples)/*\" --target ES6 --mode file --json build/docs/api/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "clean": "trash build test",
    "storybook": "start-storybook -p 6006",
    "prepublishOnly": "run-s build",
    "build-storybook": "build-storybook -o build/docs"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "memoize-one": "^5.1.1",
    "query-string": "^6.9.0"
  },
  "peerDependencies": {
    "history": "^4",
    "immer": ">=7",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "zustand": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@storybook/addon-actions": "^5.2.7",
    "@storybook/addon-docs": "^5.2.7",
    "@storybook/addon-links": "^5.2.7",
    "@storybook/addons": "^5.2.7",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.7",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "browser-env": "^3.3.0",
    "codecov": "^3.7.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "expect": "^24.9.0",
    "gh-pages": "^2.1.1",
    "history": "^4.10.1",
    "immer": "^8.0.1",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^9.0.0",
    "jsdom": "^15.2.1",
    "npm-run-all": "^4.1.5",
    "open-cli": "^5.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "semantic-release": "^17.0.4",
    "trash-cli": "^3.0.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typedoc": "^0.15.4",
    "typescript": "^4.1.3",
    "zustand": "^2.2.3"
  },
  "resolutions": {
    "lodash": "4.17.19",
    "dot-prop": "5.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/__tests__/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "roots": [
      "src/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
